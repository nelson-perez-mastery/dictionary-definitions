{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        
        "sourceMaps": true,
        "request": "launch",
        "name": "Launch Program",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/dist/main.js",
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "env": { "NODE_ENV": "development" },
        "resolveSourceMapLocations": [
          "${workspaceFolder}/**",
          "!**/node_modules/**"
        ],
        "preLaunchTask": "prestart"
      }
    ]
  }
